{
  "name":        "dreamfactory/df-firebird",
  "description": "Firebird SQL Database support for DreamFactory",
  "keywords":    [
    "dreamfactory",
    "database",
    "sql",
    "firebird",
    "pdo_firebird",
    "pdo_interbase"
  ],
  "homepage":    "https://www.dreamfactory.com/",
  "license":     "Apache-2.0",
  "authors":     [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    },
    {
      "name":  "Lee Hicks",
      "email": "leehicks@dreamfactory.com"
    }
  ],
  "support":     {
    "email":  "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-firebird",
    "issues": "https://github.com/dreamfactorysoftware/df-firebird/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/yaroslavmo/df-sqldb"
    }
  ],
  "require":     {
    "dreamfactory/df-sqldb": "~0.16.0"
  },
  "autoload":    {
    "psr-4": {
      "DreamFactory\\Core\\Firebird\\": "src/"
    }
  },
  "extra":       {
    "branch-alias": {
      "dev-develop": "0.7.x-dev"
    },
    "laravel":      {
      "providers": [
        "DreamFactory\\Core\\Firebird\\ServiceProvider"
      ]
    }
  }
}
